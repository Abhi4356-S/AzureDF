{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mydatafactory800"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDatabricks_SalesData_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks_SalesData'"
		},
		"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://datalake800.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "7282dbaf-e86c-4be9-8b43-fbe21adcee57"
		},
		"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "7e232508-abec-468b-b201-7eace19d84a8"
		},
		"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "TM_Sindhu"
		},
		"Triggernotebook_properties_DataBricks_parameters_Name": {
			"type": "string",
			"defaultValue": "@pipeline().parameters.Name"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDatabricks_SalesData_accessToken": {
						"value": "[parameters('AzureDatabricks_SalesData_accessToken')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_resourceGroupName')]"
					},
					"Triggernotebook_properties_DataBricks_parameters_Name": {
						"value": "[parameters('Triggernotebook_properties_DataBricks_parameters_Name')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}